{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.0.4",
    "manifest_version": 3,
    "key": "",
    "name": "Easy My Fpoly",
    "description": "Tiện ích trợ giúp trên hệ thống của trường cao đẳng FPT Polytechnic!",
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
        "default_icon": {
            "128": "/images/icon.png"
        }
    },
    "icons": {
        "128": "/images/icon.png"
    },
    "externally_connectable": {
        "matches": [
            "https://lms.poly.edu.vn/*"
        ]
    },
    "permissions": [
        "storage",
        "management"
    ],
    "host_permissions": [
        "*://*.poly.edu.vn/*"
    ]
}